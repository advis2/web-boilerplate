{
  "name": "web-boilerplate",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://advis2.github.io/web-boilerplate",
  "dependencies": {
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "jekyll": "^3.0.0-beta1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "three": "^0.130.1",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "gh-pages -b storybook-page -d storybook-static"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "gh-pages": "^3.2.3"
  },
  "pnpm": {
    "overrides": {
      "node-forge@<1.0.0": ">=1.0.0",
      "node-forge@<1.3.0": ">=1.3.0",
      "parse-path@<5.0.0": ">=5.0.0",
      "parse-url@<8.1.0": ">=8.1.0",
      "ejs@<3.1.7": ">=3.1.7",
      "trim@<0.0.3": ">=0.0.3",
      "browserslist@>=4.0.0 <4.16.5": ">=4.16.5",
      "loader-utils@>=2.0.0 <2.0.3": ">=2.0.3",
      "postcss@<8.4.31": ">=8.4.31",
      "ansi-html@<0.0.8": ">=0.0.8",
      "glob-parent@<5.1.2": ">=5.1.2",
      "loader-utils@>=2.0.0 <2.0.4": ">=2.0.4",
      "nth-check@<2.0.1": ">=2.0.1",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "webpack-dev-middleware@<=5.3.3": ">=5.3.4",
      "gh-pages@<5.0.0": ">=5.0.0",
      "immer@>=7.0.0 <9.0.6": ">=9.0.6",
      "ejs@<3.1.10": ">=3.1.10"
    }
  }
}
